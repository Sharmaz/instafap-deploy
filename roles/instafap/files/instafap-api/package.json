{
  "name": "instafap-api",
  "version": "1.0.0",
  "description": "InstaFap REST API",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && NODE_ENV=test ava --require babel-register test/**-test.js",
    "pictures": "micro -p 5000 pictures.js",
    "users": "micro -p 5001 users.js",
    "auth": "micro -p 5002 auth.js"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime",
      "transform-async-to-generator"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sharmaz/instafap-api.git"
  },
  "author": "Ivan Robles <irae45@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sharmaz/instafap-api/issues"
  },
  "homepage": "https://github.com/Sharmaz/instafap-api#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-eslint": "^7.1.0",
    "babel-register": "^6.18.0",
    "standard": "^8.5.0",
    "test-listen": "^1.0.0"
  },
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "gravatar": "^1.6.0",
    "http-hash": "^2.0.0",
    "instafap-db": "file:../instafap-db",
    "jsonwebtoken": "^7.1.9",
    "micro": "^4.1.1",
    "request": "^2.78.0",
    "request-promise": "^4.1.1",
    "token-extractor": "^0.1.6",
    "uuid-base62": "^0.1.0"
  }
}
